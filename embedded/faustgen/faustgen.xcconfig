C74SUPPORT = $(HOME)/Dev/SDKs/Cycling74/6.1.4/c74support
HEADER_SEARCH_PATHS = ../../architecture /usr/local/include/ /opt/local/include/ $(LLVM_PATH)/include "$(C74SUPPORT)/max-includes" "$(C74SUPPORT)/msp-includes"
FRAMEWORK_SEARCH_PATHS = "$(C74SUPPORT)/max-includes" "$(C74SUPPORT)/msp-includes"
