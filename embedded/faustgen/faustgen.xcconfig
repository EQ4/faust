GCC_PREPROCESSOR_DEFINITIONS = $(LLVM_DEFS) FAUSTGEN_VERSION='"$(PRODUCT_VERSION)"' _GNU_SOURCE __STDC_CONSTANT_MACROS __STDC_FORMAT_MACROS __STDC_LIMIT_MACROS "DENORM_WANT_FIX = 1" "NO_TRANSLATION_SUPPORT = 1"
GCC_PREFIX_HEADER = $(C74SUPPORT)/max-includes/macho-prefix.pch

C74_SYM_LINKER_FLAGS = @$(C74SUPPORT)/max-includes/c74_linker_flags.txt
LD_FLAGS = -Wl,-U,_object_method_imp -Wl,-U,_object_new_imp -lcrypto -lncurses -lz -lfaust $(LLVM_LIBS) $(C74_SYM_LINKER_FLAGS)